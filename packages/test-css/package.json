{
  "name": "test-css",
  "version": "1.0.0",
  "description": "Test-css",
  "author": "",
  "license": "UNLICENSED",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rimraf dist && concurrently \"pnpm build:code\" \"pnpm build:types\"",
    "build:code": "rollup -c",
    "build:types": "tsc --declaration true --emitDeclarationOnly --outDir dist/types"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^7",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "rimraf": "^4",
    "rollup": "^3",
    "rollup-plugin-postcss": "^4",
    "tslib": "^2",
    "typescript": "^4"
  }
}
